        "module": "commonjs",
        "noImplicitAny": false,
        "target": "es5",
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "jsx": "react",
        "baseUrl": ".",
        "lib": [
            "es5",
            "es6",
            "dom",
        ],
        "paths": {
            "*": [
                "node_modules/*"
            ]
        }
